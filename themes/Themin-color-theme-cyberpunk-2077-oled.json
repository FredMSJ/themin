	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "#43444D",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant"],
			"settings": {
				"foreground": "#9370db"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.charcter.escape"
			],
			"settings": {
				"foreground": "#FDF500"
			}
		},
		{
			"scope": ["entity.name"],
			"settings": {
				"foreground": "#FDF500"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"entity.name.tag",
				"entity.name.namespace",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#FF003C"
			}
		},
		{
			"scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
			"settings": {
				"foreground": "#fdfdfe"
			}
		},
		{
			"scope": ["invalid"],
			"settings": {
				"foreground": "#e455ae"
			}
		},
		{
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#575861"
			}
		},
		{
			"scope": ["keyword"],
			"settings": {
				"foreground": "#9370db"
			}
		},
		{
			"scope": ["meta.tag", "meta.brace"],
			"settings": {
				"foreground": "#868690"
			}
		},
		{
			"scope": ["meta.import", "meta.export"],
			"settings": {
				"foreground": "#9370db"
			}
		},
		{
			"scope": "meta.directive.vue",
			"settings": {
				"foreground": "#fdfdfe"
			}
		},
		{
			"scope": "meta.property-name.css",
			"settings": {
				"foreground": "#FF003C"
			}
		},
		{
			"scope": "meta.property-value.css",
			"settings": {
				"foreground": "#9898a6"
			}
		},
		{
			"scope": "meta.tag.other.html",
			"settings": {
				"foreground": "#575861"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "#575861"
			}
		},
		{
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "#9370db"
			}
		},
		{
			"scope": ["punctuation.definition.string"],
			"settings": {
				"foreground": "#9898a6"
			}
		},
		{
			"scope": ["punctuation.definition.tag"],
			"settings": {
				"foreground": "#43444D"
			}
		},
		{
			"scope": ["punctuation.definition.variable.php"],
			"settings": {
				"foreground": "#868690"
			}
		},
		{
			"scope": ["storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#9370db"
			}
		},
		{
			"scope": ["string"],
			"settings": {
				"foreground": "#9898a6"
			}
		},
		{
			"scope": ["support"],
			"settings": {
				"foreground": "#FF003C"
			}
		},
		{
			"scope": ["support.constant"],
			"settings": {
				"foreground": "#9898a6"
			}
		},
		{
			"scope": ["support.function"],
			"settings": {
				"foreground": "#e455ae"
			}
		},
		{
			"scope": ["variable"],
			"settings": {
				"foreground": "#FDF500"
			}
		},
		{
			"scope": [
				"variable.other",
				"variable.language",
				"variable.function",
				"variable.argument"
			],
			"settings": {
				"foreground": "#868690"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "#fdfdfe"
			}
		},
		{
			"name": "Embed section",
			"scope": [
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#00F0FF"
			}
		},
		{
			"name": "Template String",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#e455ae"
			}
		}
	]
}